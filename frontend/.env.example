# Supabase Configuration
# Replace with your own project values from Supabase Dashboard
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_public_key

# n8n Webhook Configuration
# Replace with your n8n RAG Chatbot webhook URL
VITE_N8N_RAG_WEBHOOK_URL=your_n8n_webhook_url

# Claude AI Configuration
# Get your API key from: https://console.anthropic.com/
# Used for Farmer Chat Assistant and other AI features
VITE_CLAUDE_API_KEY=your_claude_api_key_here

# AI API Configuration (Alternative - OpenRouter, etc.)
VITE_AI_API_URL=https://openrouter.ai/api/v1
VITE_AI_API_KEY=your_ai_api_key
VITE_AI_MODEL=x-ai/grok-2-1212
VITE_AI_MAX_TOKENS=2000
VITE_AI_TEMPERATURE=0.7
